public class ProductInDeal {
    String product_id, stored_price;
    double amount, price;

    public ProductInDeal(String product_id, String stored_price, double amount, double price) {
        this.product_id = product_id;
        this.stored_price = stored_price;
        this.amount = amount;
        this.price = price;
    }

    public String getProduct_id() {
        return product_id;
    }

    public void setProduct_id(String product_id) {
        this.product_id = product_id;
    }

    public String getStored_price() {
        return stored_price;
    }

    public void setStored_price(String stored_price) {
        this.stored_price = stored_price;
    }

    public double getAmount() {
        return amount;
    }

    public void setAmount(double amount) {
        this.amount = amount;
    }

    public double getPrice() {
        return price;
    }

    public void setPrice(double price) {
        this.price = price;
    }
}
