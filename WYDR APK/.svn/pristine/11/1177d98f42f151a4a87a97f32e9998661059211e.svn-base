<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="15dp"
            android:background="#fff">
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="0px"
                android:focusable="true"
                android:focusableInTouchMode="true" />
            <TextView
                android:id="@+id/personalDetail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:paddingBottom="6dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="4dp"
                android:text="@string/personal_details"
                android:textColor="@color/text_color"
                android:textSize="16dp"
                tools:ignore="RelativeOverlap" />

            <ImageView
                android:id="@+id/imgPersonal"
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/personalDetail"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/personalDetail"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="10dp"
                android:src="@drawable/up_arrow_with_padding" />

            <LinearLayout
                android:id="@+id/linearpersonal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/personalDetail"
                android:orientation="vertical">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputUsername"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/username"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:drawableLeft="@drawable/bussiness"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/bussiness"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="Name *"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"
                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textEmailAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtemail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/username"
                        android:drawableLeft="@drawable/location_form"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/location_form"

                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="Email *"
                        android:inputType="textEmailAddress"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputBusiness"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtprodname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/txtemail"
                        android:drawableLeft="@drawable/bussiness"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/bussiness"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="@string/business_shop"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"
                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtaddress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/txtprodname"
                        android:drawableLeft="@drawable/location_form"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/location_form"

                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="@string/address"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputCity"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtcity"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/txtaddress"
                        android:drawableLeft="@drawable/city_form"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/city_form"

                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="@string/city"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputState"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtstate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@+id/txtcity"
                        android:drawableLeft="@drawable/city_form"
                        android:drawablePadding="15dp"
                        android:drawableStart="@drawable/city_form"

                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="@string/state"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>


                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputPin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtpin"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/txtstate"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:drawableLeft="@drawable/card"
                        android:drawableStart="@drawable/card"
                        android:drawablePadding="15dp"
                        android:editable="true"
                        android:hint="PIN CODE *"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>
            </LinearLayout>
        </RelativeLayout>




        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="15dp"
            android:background="#fff">
            <LinearLayout
                android:layout_width="0px"
                android:layout_height="0px"
                android:focusable="true"
                android:focusableInTouchMode="true" />
            <TextView
                android:id="@+id/businessDetail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:paddingBottom="6dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="4dp"
                android:text="KYC INFORMATION"
                android:textColor="@color/text_color"
                android:textSize="16dp" />

            <ImageView
                android:id="@+id/imgBuis"
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/businessDetail"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/businessDetail"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="10dp"
                android:src="@drawable/up_arrow_with_padding" />

            <LinearLayout
                android:id="@+id/linearbusiness"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/businessDetail"
                android:orientation="vertical">

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputPan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtpan"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:drawableLeft="@drawable/card"
                        android:drawableStart="@drawable/card"
                        android:drawablePadding="15dp"
                        android:editable="true"
                        android:hint="@string/pan"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"

                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputTin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txttin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@id/txtpan"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"

                        android:drawableLeft="@drawable/card"
                        android:drawableStart="@drawable/card"
                        android:drawablePadding="15dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="@string/tin"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"
                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/textInputVat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/activegraybar"

                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                    >
                    <EditText
                        android:id="@+id/txtvat"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_below="@id/txtpan"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="8dp"

                        android:drawableLeft="@drawable/card"
                        android:drawableStart="@drawable/card"
                        android:drawablePadding="15dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="8dp"
                        android:background="@drawable/buiscat_bck"
                        android:editable="true"
                        android:hint="VAT"
                        android:inputType="textNoSuggestions"
                        android:padding="3dp"
                        android:textColor="@color/text_color"
                        android:textColorHint="@color/divider"
                        android:textCursorDrawable="@drawable/color_cursor"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        android:typeface="sans" />
                </android.support.design.widget.TextInputLayout>

                <RadioGroup
                    android:id="@+id/radiobankingdetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="false"
                    android:layout_below="@+id/txtvat"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginTop="7dp"
                    android:orientation="horizontal"
                    android:background="@drawable/round_bg">

                    <RadioButton
                        android:id="@+id/radioSeller"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/checked_form"
                        android:checked="true"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="Seller Bank Details"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />


                    <RadioButton
                        android:id="@+id/radioNotSeller"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/unchecked_form"

                        android:checked="false"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="I don't want to sell"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />
                </RadioGroup>

                <LinearLayout
                    android:id="@+id/linearaccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView7"
                    android:orientation="vertical">

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/textAccountHolderName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/activegraybar"

                        android:orientation="vertical"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                        >
                        <EditText
                            android:id="@+id/txtAccName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_below="@+id/radiobankingdetails"
                            android:drawableLeft="@drawable/location_form"
                            android:drawablePadding="15dp"
                            android:drawableStart="@drawable/location_form"

                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="8dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="8dp"
                            android:background="@drawable/buiscat_bck"
                            android:editable="true"
                            android:hint="Account Holder Name *"
                            android:inputType="textNoSuggestions"
                            android:padding="3dp"

                            android:textColor="@color/text_color"
                            android:textColorHint="@color/divider"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:textSize="14dp"
                            android:textStyle="normal"
                            android:typeface="sans" />
                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/textAccountNumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/activegraybar"

                        android:orientation="vertical"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                        >
                        <EditText
                            android:id="@+id/txtaccno"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:drawableLeft="@drawable/bussiness"
                            android:drawablePadding="15dp"
                            android:drawableStart="@drawable/bussiness"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="8dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="8dp"
                            android:layout_below="@+id/txtAccName"
                            android:background="@drawable/buiscat_bck"
                            android:editable="true"
                            android:hint="Account Number *"
                            android:inputType="textNoSuggestions"
                            android:padding="3dp"
                            android:textColor="@color/text_color"
                            android:textColorHint="@color/divider"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:textSize="14dp"
                            android:textStyle="normal"
                            android:typeface="sans" />
                    </android.support.design.widget.TextInputLayout>
                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/textInputBank"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/activegraybar"

                        android:orientation="vertical"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                        >
                        <EditText
                            android:id="@+id/txtbank"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_below="@+id/txtaccno"
                            android:drawableLeft="@drawable/location_form"
                            android:drawablePadding="15dp"
                            android:drawableStart="@drawable/location_form"

                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="8dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="8dp"
                            android:background="@drawable/buiscat_bck"
                            android:editable="true"
                            android:hint="Bank Name *"
                            android:inputType="textNoSuggestions"
                            android:padding="3dp"

                            android:textColor="@color/text_color"
                            android:textColorHint="@color/divider"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:textSize="14dp"
                            android:textStyle="normal"
                            android:typeface="sans" />
                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/textIfsc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/activegraybar"

                        android:orientation="vertical"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
                        >
                        <EditText
                            android:id="@+id/txtIfscCode"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:drawableLeft="@drawable/city_form"
                            android:drawablePadding="15dp"
                            android:drawableStart="@drawable/city_form"
                            android:layout_below="@+id/txtbank"

                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="8dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="8dp"
                            android:background="@drawable/buiscat_bck"
                            android:editable="true"
                            android:hint="IFSC CODE *"
                            android:inputType="textNoSuggestions"
                            android:padding="3dp"

                            android:textColor="@color/text_color"
                            android:textColorHint="@color/divider"
                            android:textCursorDrawable="@drawable/color_cursor"
                            android:textSize="14dp"
                            android:textStyle="normal"
                            android:typeface="sans" />
                    </android.support.design.widget.TextInputLayout>

                    <RadioGroup
                        android:id="@+id/radioaccounttype"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="false"
                        android:layout_below="@+id/txtIfscCode"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="7dp"
                        android:layout_marginRight="7dp"
                        android:layout_marginTop="7dp"
                        android:orientation="horizontal"
                        android:background="@drawable/round_bg">

                        <RadioButton
                            android:id="@+id/radioSaving"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginLeft="8dp"
                            android:layout_weight="1"
                            android:button="@drawable/checked_form"
                            android:checked="true"
                            android:paddingBottom="6dp"
                            android:paddingLeft="3dp"
                            android:paddingRight="6dp"
                            android:paddingTop="6dp"
                            android:text="Saving"
                            android:textColor="@color/text_color"
                            android:textSize="13dp" />


                        <RadioButton
                            android:id="@+id/radioCurrent"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_weight="1"
                            android:button="@drawable/unchecked_form"

                            android:checked="false"
                            android:paddingBottom="6dp"
                            android:paddingLeft="3dp"
                            android:paddingRight="6dp"
                            android:paddingTop="6dp"
                            android:text="Current"
                            android:textColor="@color/text_color"
                            android:textSize="13dp" />
                    </RadioGroup>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>




        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff">


            <TextView

                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"

                android:layout_marginBottom="6dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:paddingBottom="6dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="4dp"
                android:text="@string/market_refernence"
                android:textColor="@color/text_color"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/imgMarket"
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/textView7"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/textView7"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="10dp"
                android:src="@drawable/up_arrow_with_padding" />

            <LinearLayout
                android:id="@+id/linearadditional"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView7"
                android:orientation="vertical">

                <RadioGroup
                    android:id="@+id/radiogrp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="false"
                    android:layout_below="@+id/textView7"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginTop="7dp"
                    android:orientation="horizontal"
                    android:background="@drawable/round_bg">

                    <RadioButton
                        android:id="@+id/radioButton"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/checked_form"
                        android:checked="true"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="@string/_public"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />


                    <RadioButton
                        android:id="@+id/radioButton2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/unchecked_form"

                        android:checked="false"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="@string/_private"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />
                </RadioGroup>

                <TextView
                    android:id="@+id/radioText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/radiogrp"
                    android:layout_centerHorizontal="true"

                    android:elegantTextHeight="false"
                    android:padding="12dp"
                    android:text="@string/busidetpub"
                    android:textColor="@color/text_color"
                    android:textSize="13dp"
                    android:textStyle="normal" />

                <LinearLayout
                    android:id="@+id/linearbuis"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/radioText"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <wydr.sellers.slider.SuperSpinner
                        android:id="@+id/spinner1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:entries="@array/category_arrays"
                        android:background="@null"
                        android:paddingTop="0dp"

                        />
                </LinearLayout>

                <TextView
                    android:id="@+id/txtintersted"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/linearbuis"
                    android:layout_marginBottom="2dp"
                    android:drawableLeft="@drawable/intrsted"
                    android:drawableStart="@drawable/intrsted"
                    android:drawablePadding="15dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="8dp"
                    android:layout_weight="1"
                    android:editable="true"

                    android:gravity="left"
                    android:paddingBottom="3dp"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp"
                    android:paddingTop="3dp"
                    android:text="@string/interested_in"
                    android:textColor="@color/text_color"
                    android:textColorHint="@color/divider"
                    android:textStyle="normal"
                    android:typeface="sans" />

                <RadioGroup
                    android:id="@+id/radiogrp2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="false"
                    android:background="@drawable/round_bg"

                    android:layout_below="@+id/txtintersted"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="7dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/radioBuy"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/checked_form"
                        android:checked="true"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="@string/buy"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />

                    <RadioButton
                        android:id="@+id/radioSell"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/unchecked_form"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="@string/sell"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />

                    <RadioButton
                        android:id="@+id/radioBoth"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="1"
                        android:button="@drawable/unchecked_form"
                        android:paddingBottom="6dp"
                        android:paddingLeft="3dp"
                        android:paddingRight="6dp"
                        android:paddingTop="6dp"
                        android:text="@string/both"
                        android:textColor="@color/text_color"
                        android:textSize="13dp" />
                </RadioGroup>

            </LinearLayout>

        </RelativeLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="10">

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sellerAgreement"
                android:layout_weight=".1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="9.9"
                android:id="@+id/sellerAgreement_tv"
                android:textColor="@color/text_color"
                android:textSize="13dp"
                android:textStyle="normal" />

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:baselineAligned="false"
            android:orientation="horizontal"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="40"
                android:gravity="center"

                >

                <Button
                    android:id="@+id/btnskip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/skip_button"
                    android:gravity="center"
                    android:padding="6dp"
                    android:text="@string/skip_now"
                    android:textColor="#fff"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="60"
                android:gravity="center">

                <Button
                    android:id="@+id/sub_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/submit_button_buisness"
                    android:gravity="center"
                    android:padding="6dp"
                    android:text="@string/submit"
                    android:textColor="#fff"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>

        <!--  <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal">


          </LinearLayout>-->


    </LinearLayout>
</ScrollView>