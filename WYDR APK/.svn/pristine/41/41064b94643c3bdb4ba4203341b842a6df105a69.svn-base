public class AddNewConnection {

    private String userId;
    private String mobileNumber;

    public AddNewConnection(String userId, String mobileNumber) {
        this.userId = userId;
        this.mobileNumber = mobileNumber;
    }

    public void setMobileNumber(String userId, String mobileNumber) {
        this.userId = userId;
        this.mobileNumber = mobileNumber;
    }

    public String getuserId() {
        return userId;
    }

    public String getMobileNumber() {
        return mobileNumber;
    }


}
